{
  "name": "gym-manager",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all -p nodemon browser-sync",
    "nodemon": "nodemon server.js",
    "browser-sync": "browser-sync start --proxy http://localhost:5000 --files 'public, views'"
  },
  "dependencies": {
    "express": "^4.17.1",
    "nunjucks": "^3.2.1"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5"
  },
  "nodemonConfig": {
    "ignore": [
      "data.json"
    ]
  }
}